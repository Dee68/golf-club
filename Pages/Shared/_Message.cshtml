@if (TempData["success"] != null)
{
    <div class=" alert alert-dismissible alert-success my-5 text-center">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        @TempData["success"]
    </div>
}
@if (TempData["error"] != null)
{
    <div class=" alert alert-dismissible alert-danger my-5 text-center">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        @TempData["error"]
    </div>
}
